@* ckfinder *@

@* ckeditor *@
<script language="javascrip" type="text/javascript">
    var ckeditor;
    function createEditer(langguageCode, id) {
        var ckeditor = CKEDITOR.replace(id, { langguage: langguageCode });


    };
    $(function () {
        createEditer('vi', 'Content');
    });


</script>










